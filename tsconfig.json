{
  // extend Kibana's tsconfig, or use your own settings
  "extends": "../../tsconfig.json",
  // tell the TypeScript compiler where to find your source files
  "include": [
    "lib/**/*",
    "server/**/*",
    "common/**/*",
    "public/**/*"
  ],
  "exclude": [
    "public/**/*.test.ts",
    "public/**/*.test.tsx",
    "lib/**/*.test.ts",
    "server/**/*.test.ts",
    "common/**/*.test.ts"
  ]
}

