{
  "name": "searchguard",
  "version": "7.10.0-master-SNAPSHOT",
  "description": "Search Guard features for Kibana",
  "main": "index.js",
  "homepage": "https://search-guard.com",
  "license": "Apache-2.0",
  "searchguard": {
    "test_sg_version": "7.9.2-master-SNAPSHOT",
    "kibana_branch": "7.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/floragunncom/search-guard-kibana-plugin"
  },
  "scripts": {
    "build:bundles": "node ../../scripts/build_kibana_platform_plugins.js --verbose --no-examples",
    "lint": "../../node_modules/.bin/eslint .",
    "kbn": "node ../../scripts/kbn",
    "test:browser": "./node_modules/.bin/jest --clearCache && ../../node_modules/.bin/jest --testPathIgnorePatterns=server  --config ./tests/jest.config.js --silent",
    "test:server": "./node_modules/.bin/jest --clearCache && ../../node_modules/.bin/jest --testPathIgnorePatterns=public --config ./tests/jest.config.js --silent",
    "test:prepare_integration_test": "yarn && ./patches/patch_kibana_es_version_compatibility.sh",
    "patch:kibana:samesite": "./patches/patch_to_add_samesite_none_to_cookies.sh",
    "patch:kibana:dev:es_compatibility": "./patches/patch_kibana_es_version_compatibility.sh",
    "patch:kibana:dev": "./patches/patch_kibana.sh dev",
    "patch:kibana": "./patches/patch_kibana.sh"
  },
  "dependencies": {
    "@elastic/kibana-ui-framework": "0.0.11",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "boom": "^7.3.0",
    "brace": "^0.11.1",
    "cryptiles": "4.x.x",
    "dompurify": "^2.0.11",
    "formik": "1.5.8",
    "formik-effect": "^1.2.0",
    "hapi-auth-cookie": "^9.1.0",
    "joi": "10.6.0",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "mustache": "^3.0.3",
    "prop-types": "^15.7.2",
    "raw-loader": "^3.1.0",
    "react-ace": "^7.0.4",
    "react-motion": "^0.5.2",
    "react-router-dom": "^5.0.0",
    "react-vis": "^1.11.7",
    "requirefrom": "^0.2.0",
    "uuid": "^3.3.2",
    "wreck": "14.x.x"
  },
  "devDependencies": {
    "core-js": "^3.6.5",
    "enzyme-to-json": "^3.5.0",
    "jest": "^26.5.2",
    "ts-jest": "^26.4.1"
  }
}
